generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id           Int     @id @default(autoincrement())
  email        String  @unique
  password     String
  nama         String?
  level        String
  company      String
  no_telpon    String
  accessToken  String  @default("NULL")
  refreshToken String  @default("NULL")
}

model akunheaders {
  id               Int    @id @default(autoincrement())
  kode_akun_header String @unique
  nama_akun_header String
}

model akundetail {
  id               Int    @id @default(autoincrement())
  company          String
  kode_akun_header String
  kode_akun        String
  nama_akun        String
}
